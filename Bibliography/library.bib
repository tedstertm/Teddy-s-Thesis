journal = {Journal of laboratory automation},
keywords = {Animals,Antibodies,Antibodies: analysis,Antibodies: isolation {\&} purification,Automation, Laboratory,Chemistry Techniques, Analytical,Complex Mixtures,Complex Mixtures: chemistry,Filtration,Filtration: instrumentation,Filtration: methods,High-Throughput Screening Assays,Humans,Magnetic Phenomena,Microarray Analysis,Microarray Analysis: utilization,Surface Tension},
month = {jun},
