language = {en},
month = {dec},
number = {12},
pages = {1355--9},
pmid = {16286965},
publisher = {The Royal Society of Chemistry},
title = {{Continuous flow in open microfluidics using controlled evaporation.}},
url = {http://pubs.rsc.org/en/content/articlehtml/2005/lc/b510044e},
volume = {5},
year = {2005}
