keywords = {Cell Adhesion,Drug Resistance, Neoplasm,Humans,Integrins,Integrins: metabolism,Multiple Myeloma,Multiple Myeloma: drug therapy,Multiple Myeloma: metabolism,Multiple Myeloma: pathology},
month = {jun},
number = {1-2},
