pages = {442--53},
pmid = {11948451},
title = {{Efficiency of embryoid body formation and hematopoietic development from embryonic stem cells in different culture systems.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/11948451},
volume = {78},
year = {2002}
}
