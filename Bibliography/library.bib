pages = {301ra130--301ra130},
title = {{Obesity-dependent changes in interstitial ECM mechanics promote breast tumorigenesis}},
url = {http://stm.sciencemag.org/content/7/301/301ra130.full-text.pdf+html},
volume = {7},
year = {2015}
}
