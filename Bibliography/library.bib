issn = {1465-6906},
journal = {Genome Biology},
number = {7},
pages = {R89},
title = {{Host lifestyle affects human microbiota on daily timescales}},
url = {http://genomebiology.com/2014/15/7/R89},
volume = {15},
year = {2014}
}
