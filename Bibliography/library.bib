journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {Adrenal Cortex,Adrenal Cortex: cytology,Biological,Breast Neoplasms,Breast Neoplasms: pathology,Capillaries,Capillaries: metabolism,Cell Biology,Cell Biology: instrumentation,Cell Line,Cell Membrane,Cell Membrane: physiology,Cell Movement,Collagen Type I,Collagen Type I: metabolism,Computer Simulation,Female,Humans,Hydrocortisone,Hydrocortisone: analysis,Hydrocortisone: metabolism,Male,Matrix Metalloproteinase Inhibitors,Matrix Metalloproteinase Inhibitors: pharmacology,Metabolomics,Metabolomics: instrumentation,Metabolomics: methods,Microfluidic Analytical Techniques,Microfluidics,Microfluidics: instrumentation,Microfluidics: methods,Models,Prostatic Neoplasms,Prostatic Neoplasms: pathology,Steroids,Steroids: analysis,Steroids: metabolism,Toxicology,Toxicology: instrumentation,Toxicology: methods,Tumor},
