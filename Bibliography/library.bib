journal = {PloS one},
keywords = {Animals,Antigens, CD11b,Antigens, CD11b: metabolism,Bone and Bones,Bone and Bones: drug effects,Bone and Bones: metabolism,Bone and Bones: pathology,Cell Differentiation,Cell Differentiation: drug effects,Cell Line, Tumor,Diphosphonates,Diphosphonates: pharmacology,Female,Humans,Imidazoles,Imidazoles: pharmacology,Mice,Multiple Myeloma,Multiple Myeloma: metabolism,Multiple Myeloma: pathology,Myeloid Cells,Myeloid Cells: pathology,Osteoclasts,Osteoclasts: drug effects,Osteoclasts: metabolism,Osteoclasts: pathology,Prenylation,Prenylation: drug effects,Tumor Microenvironment,Tumor Microenvironment: drug effects},
