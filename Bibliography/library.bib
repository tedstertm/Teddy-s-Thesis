month = {mar},
number = {3},
pages = {624--7},
pmid = {14749707},
publisher = {Nature Publishing Group},
shorttitle = {Leukemia},
title = {{Effect of thalidomide therapy on bone marrow angiogenesis in multiple myeloma.}},
