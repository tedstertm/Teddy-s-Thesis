@article{Muir1958,
abstract = {Methods were studied for the preparation, isolation, and growth of single cells into cultures. The first step was to secure actively growing cells. Marigold tissue of crown gall origin grew well in liquid on a shaker when the initial pH was from 4.6 to 7.0. Tissues of several species grew well in liquid under several aeration conditions. The growth rates of liquid marigold cultures on a shaker were increased by up to 0.02 per cent yeast extract and were inhibited above 0.04 per cent. Single cells were isolated from diffuse liquid cultures and friable agar cultures. Attempts to grow single cells on agar media, on filter paper over agar media, or in sealed hanging drops of "acclimated" medium generally were unsuccessful. Single cells grew successfully on filter paper over established cultures. Most cultures of single cell origin (clones) grew well after transfer to agar, but some did not. Several were carried through 25 or more agar transfers, each of 5 to 6 weeks duration, without diminution in growth. Clones were established from single cells of marigold and grape of crown gall origin and from normal N. tabacum (strain WTI), N. glutinosa x N. tabacum (strain WTH), and marigold. Constant differences were observed among clones derived from the same parent stock of marigold (crown gall origin) in growth rate, texture, and color. Some clones with varied media have developed shoots or roots in culture. CR  - Copyright {\&}{\#}169; 1958 Botanical Society of America},
author = {Muir, W H and Hildebrandt, A C and Riker, A J},
doi = {10.2307/2439231},
issn = {00029122},
journal = {American Journal of Botany},
month = {oct},
number = {8},
pages = {589--597},
publisher = {Botanical Society of America},
title = {{The Preparation, Isolation, and Growth in Culture of Single Cells from Higher Plants}},
url = {http://www.jstor.org/stable/2439231},
volume = {45},
year = {1958}
}
